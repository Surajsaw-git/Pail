;10. Write a program to multiply two 8-bit numbers and display the result on the console.

section .text
global _start
_start:
    mov al,3
    mov bl,4
    mul bl
    add al,'0'
    mov [res],al
    mov eax,4
    mov ebx,1
    mov ecx,res
    mov edx,1
    int 0x80
    mov eax,1
    int 0x80
section .data
res db 0
